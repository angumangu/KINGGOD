pipeline{
    agent any
    stages{
        stage("git checkout"){
            steps{
                git branch: 'main', url: 'https://github.com/mrakbg/KINGGOD.git'
            }
        }
        stage("Docker remove image"){
            steps{
                script{
                    sh"docker rmi -f mrakbg/3tierscratch:v4 || true"
                    sh"docker build -t mrakbg/3tierscratch:v4 ."
                    sh"docker push mrakbg/3tierscratch:v4"

                    
                }

            }
        }
    }
    
}